<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclure les données sensibles du backup cloud -->
        <exclude domain="sharedpref" path="fcm_prefs.xml"/>
        <exclude domain="database" path="*.db"/>
    </cloud-backup>
    <device-transfer>
        <!-- Autoriser le transfert de certaines données lors du changement d'appareil -->
        <include domain="sharedpref" path="l2l_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>
