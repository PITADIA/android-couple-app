    "NSLocationWhenInUseUsageDescription" : {
      "comment" : "Location usage description",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love uses your location to calculate the distance between you and your partner, and to allow you to add places to your shared memories."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love utilise votre localisation pour calculer la distance entre vous et votre partenaire, et pour vous permettre d'ajouter des lieux à vos souvenirs partagés."
          }
        },
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love nutzt deinen Standort, um die Entfernung zwischen dir und deinem Partner bzw. deiner Partnerin zu berechnen und damit du Orte zu euren gemeinsamen Erinnerungen hinzufügen kannst."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love utiliza tu ubicación para calcular la distancia entre tú y tu pareja, y para permitirte añadir lugares a vuestros recuerdos compartidos."
          }
        }
      }
    },
    "NSPhotoLibraryUsageDescription" : {
      "comment" : "Photo library usage description",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love needs access to your photos to allow you to personalize your profile and add images to your shared memories in the couple's journal."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love a besoin d'accéder à vos photos pour vous permettre de personnaliser votre profil et d'ajouter des images à vos souvenirs partagés dans le journal de couple."
          }
        },
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love benötigt Zugriff auf deine Fotos, damit du dein Profil personalisieren und Bilder zu euren gemeinsamen Erinnerungen im Paar-Tagebuch hinzufügen kannst."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love necesita acceder a tus fotos para permitirte personalizar tu perfil y añadir imágenes a tus recuerdos compartidos en el diario de pareja."
          }
        }
      }
    },
    "NSUserNotificationsUsageDescription" : {
      "comment" : "Notification usage description",
      "extractionState" : "stale",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love sends you notifications when your partner sends you a message. You can disable these notifications at any time in Settings."
          }
        },
        "fr" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love vous envoie des notifications lorsque votre partenaire vous écrit un message. Vous pouvez désactiver ces notifications à tout moment dans les Réglages."
          }
        },
        "de" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love sendet dir Benachrichtigungen, wenn dein Partner bzw. deine Partnerin dir schreibt. Du kannst diese Benachrichtigungen jederzeit in den Einstellungen deaktivieren."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Love2Love te envía notificaciones cuando tu pareja te envía un mensaje. Puedes desactivar estas notificaciones en cualquier momento en Ajustes."
          }
        }
      }
    }
  },
  "version" : "1.0"
